{
    "name": "huang-yi/rbac-laravel",
    "description": "A RBAC package for Laravel.",
    "keywords": ["rbac", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Huang Yi",
            "email": "coodeer@163.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.0",
        "illuminate/database": "~5.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.3.*",
        "phpunit/phpunit": "~4.8|~5.0"
    },
    "autoload": {
        "psr-4": {
            "HuangYi\\Rbac\\": "src"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HuangYi\\Rbac\\Tests\\": "tests"
        }
    }
}
